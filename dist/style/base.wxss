@charset "UTF-8";
page {
  height: 100%;
  background-color: #F5F5F5; }

.mr10 {
  margin-right: 10rpx; }

.mr20 {
  margin-right: 20rpx; }

.ml10 {
  margin-left: 10rpx; }

.ml20 {
  margin-left: 20rpx; }

.mt10 {
  margin-top: 10rpx; }

.mt20 {
  margin-top: 20rpx; }

.mb10 {
  margin-bottom: 10rpx; }

.mb20 {
  margin-bottom: 20rpx; }

text, view {
  font-family: MicroSoft Yahei;
  font-size: 28rpx;
  color: #262626; }

.form text, .form view {
  font-family: 宋体;
  font-size: 17px; }

button[type="primary"] {
  background-color: #1296DB; }

button.pain-btn[open-type="share"] {
  background-color: transparent;
  line-height: normal;
  padding: 0;
  margin: 0; }

button {
  margin: 0; }

.weui-btn.mini-btn {
  margin-top: 0; }

button.pain-btn[open-type="share"]::after {
  border: none; }

button[plain] {
  border: none; }

.button-hover[type="primary"] {
  background-color: #1086c3; }

text.white {
  color: #FFF; }

text.muted, input.muted, view.muted {
  color: #A3A3A3; }

text.weak {
  color: #626E7A; }

text.primary {
  color: #1296DB; }

text.major {
  color: #FF6200; }

text.danger {
  color: #e33; }

text.xl {
  font-size: 32rpx; }

text.xxl {
  font-size: 34rpx; }

text.lg {
  font-size: 30rpx; }

text.sm {
  font-size: 26rpx; }

text.xs {
  font-size: 24rpx; }

text.xxs {
  font-size: 22rpx; }

text.inline {
  text-overflow: ellipsis;
  word-break: keep-all;
  white-space: nowrap;
  overflow: hidden; }

text.cancel {
  font-size: 26rpx;
  color: #A3A3A3;
  text-decoration: line-through; }

view.bg-white {
  background-color: white; }

.row, .row-center, .row-between, .row-around, .row-end {
  display: flex;
  flex-direction: row;
  align-items: center; }

.row-center {
  justify-content: center; }

.row-between {
  justify-content: space-between; }

.row-around {
  justify-content: space-around; }

.row-end {
  justify-content: flex-end; }

.column, .column-center, .column-around, .column-between, .container {
  display: flex;
  justify-content: center;
  flex-direction: column; }

.column-center {
  align-items: center; }

.column-around {
  align-items: center;
  justify-content: space-around; }

.column-between {
  align-items: center;
  justify-content: space-between; }

.fixed-bottom {
  position: fixed;
  bottom: 0; }

.icon {
  min-height: 20px;
  min-width: 20px;
  height: 20px;
  width: 20px;
  vertical-align: middle; }

.icon-lg {
  min-height: 30px;
  min-width: 30px;
  height: 30px;
  width: 30px;
  vertical-align: middle; }

.icon-xl {
  min-height: 50px;
  min-width: 50px;
  height: 50px;
  width: 50px;
  vertical-align: middle; }

.icon-xxl {
  min-height: 60px;
  min-width: 60px;
  height: 60px;
  width: 60px;
  vertical-align: middle; }

.icon-round {
  border-radius: 50%; }

.hr {
  width: 100%;
  height: 6rpx; }

.h-gap {
  width: 100%;
  height: 10px; }

.tab-border {
  position: fixed;
  height: 1px;
  width: 100%;
  background-color: #E6E8EB;
  bottom: 0; }

.panel {
  background: #fff;
  border-top: 1px solid #EDEDED;
  border-bottom: 1px solid #EDEDED;
  margin-top: 10rpx;
  overflow: hidden; }

.label {
  display: inline-block;
  font-size: 12px;
  height: 28px;
  line-height: 28px;
  color: #333;
  border: 1rpx solid #999;
  padding: 0 10px;
  border-radius: 2px;
  margin-right: 10px;
  box-sizing: border-box;
  vertical-align: middle;
  text-align: center; }
  .label.primary {
    color: #fff;
    background: #1296DB;
    border: 1rpx solid #1296DB; }
  .label.disabled {
    color: #cacaca;
    background: #eee;
    border: 1rpx solid #e5e5e5; }
  .label.small {
    font-size: 11px;
    height: 20px;
    line-height: 18px;
    padding: 0 3px; }
  .label.pain.primary {
    color: #1296DB;
    background: #fff; }
